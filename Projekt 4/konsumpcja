        //************************

        //Zapis znaku do bufora
        towar = fopen("towar", "w");
        if (towar == NULL)
        {
            perror("fopen()");
            exit(1);
        }
        fputc(znak, towar);
        if (fclose(towar) == EOF)
        {
            perror("fclose()");
            exit(1);
        }
        //*********************

        //Dołączenie znaku do całkowitej produkcji (pliku "produkcja")
        fputc(znak, produkcja);
        //************************************************************

        //Pobranie nowego znaku
        znak = fgetc(surowce);
        //*********************
    }

    if (fclose(surowce) == EOF)
    {
        perror("fclose()");
        exit(1);
    }
    if (fclose(produkcja) == EOF)
    {
        perror("fclose()");
        exit(1);
    }
    return 0;
}
